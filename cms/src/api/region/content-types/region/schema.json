{
  "kind": "collectionType",
  "collectionName": "regions",
  "info": {
    "singularName": "region",
    "pluralName": "regions",
    "displayName": "Region"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "region_code": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "countries": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::country.country",
      "mappedBy": "region"
    },
    "sales_rep": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::sales-rep.sales-rep",
      "mappedBy": "region"
    }
  }
}
