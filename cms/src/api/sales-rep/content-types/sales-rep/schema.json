{
  "kind": "collectionType",
  "collectionName": "sales_reps",
  "info": {
    "singularName": "sales-rep",
    "pluralName": "sales-reps",
    "displayName": "Sales Rep"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "email": {
      "type": "email",
      "required": true,
      "unique": true
    },
    "profile_picture": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "region": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::region.region",
      "inversedBy": "sales_rep"
    }
  }
}
